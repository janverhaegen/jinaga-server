{
  "name": "jinaga-server",
  "version": "3.1.1",
  "description": "Application-agnostic back end for web applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist/ && rimraf integration-test/jinaga-test/jinaga/ && rimraf integration-test/jinaga-test/node_modules/",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "integration": "npm run build && cd ./integration-test && ./run.sh",
    "interactive": "nodemon -w docs/specification.txt -x \"cat docs/specification.txt | node interactive\"",
    "interactive:results": "nodemon -w docs/specification.txt -x \"cat docs/specification.txt | node interactive --results\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jinaga/jinaga-server.git"
  },
  "keywords": [
    "messaging"
  ],
  "author": "Michael L Perry",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jinaga/jinaga-server/issues"
  },
  "homepage": "https://jinaga.com",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/node-forge": "^0.10.10",
    "@types/passport": "^1.0.7",
    "@types/pg": "^8.6.1",
    "jest": "^28.1.0",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "express": "^4.18.1",
    "jinaga": "^4.0.0",
    "node-forge": "^1.3.1",
    "passport": "^0.6.0",
    "pg": "^8.7.1"
  }
}
